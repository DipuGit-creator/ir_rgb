check for a new IR code ===\nbasic.forever(function () {\n    if (irVS1838.wasIrDataReceived()) {\n        // ready for the next press\n        code = irVS1838.irButton()\n        // === Map each code to its button name ===\n        if (code == 162) {\n        \t\n        } else if (code == 98) {\n        \t\n        } else if (code == 226) {\n        \t\n        } else if (code == 34) {\n        \t\n        } else if (code == 2) {\n        \t\n        } else if (code == 194) {\n            pins.analogWritePin(AnalogPin.P7, 1023)\n            pins.analogWritePin(AnalogPin.P5, 1023)\n            pins.analogWritePin(AnalogPin.P6, 1023)\n            whitex = 0\n            P5x = 0\n            P6x = 0\n            P7x = 0\n            SELECTED_COLOR = 9999\n        } else if (code == 224) {\n            if (mode == 1 && SELECTED_COLOR == 0) {\n                P5x += 50\n                pins.analogWritePin(AnalogPin.P5, P5x)\n            } else if (mode == 1 && SELECTED_COLOR == 1) {\n                P6x += 50\n                pins.analogWritePin(AnalogPin.P6, P6x)\n            } else if (mode == 1 && SELECTED_COLOR == 2) {\n                P7x += 50\n                pins.analogWritePin(AnalogPin.P7, P7x)\n            } else if (mode == 1 && SELECTED_COLOR == -1) {\n                whitex += 50\n                pins.analogWritePin(AnalogPin.P5, whitex)\n                pins.analogWritePin(AnalogPin.P6, whitex)\n                pins.analogWritePin(AnalogPin.P7, whitex)\n            }\n        } else if (code == 168) {\n            if (mode == 1 && SELECTED_COLOR == 0) {\n                P5x += -50\n                pins.analogWritePin(AnalogPin.P5, P5x)\n            } else if (mode == 1 && SELECTED_COLOR == 1) {\n                P6x += -50\n                pins.analogWritePin(AnalogPin.P6, P6x)\n            } else if (mode == 1 && SELECTED_COLOR == 2) {\n                P7x += -50\n                pins.analogWritePin(AnalogPin.P7, P7x)\n            } else if (mode == 1 && SELECTED_COLOR == -1) {\n                whitex += -50\n                pins.analogWritePin(AnalogPin.P5, whitex)\n                pins.analogWritePin(AnalogPin.P6, whitex)\n                pins.analogWritePin(AnalogPin.P7, whitex)\n            }\n        } else if (code == 144) {\n            toggleMode()\n            pins.analogWritePin(AnalogPin.P7, 1023)\n            pins.analogWritePin(AnalogPin.P5, 1023)\n            pins.analogWritePin(AnalogPin.P6, 1023)\n            whitex = 0\n            P5x = 0\n            P6x = 0\n            P7x = 0\n            SELECTED_COLOR = 9999\n        } else if (code == 104) {\n            pins.analogWritePin(AnalogPin.P7, 0)\n            pins.analogWritePin(AnalogPin.P5, 0)\n            pins.analogWritePin(AnalogPin.P6, 0)\n            SELECTED_COLOR = -1\n        } else if (code == 152) {\n        \t\n        } else if (code == 176) {\n        \t\n        } else if (code == 48) {\n            if (mode == 0) {\n                pins.analogWritePin(AnalogPin.P7, 1023)\n                pins.analogWritePin(AnalogPin.P5, 0)\n                pins.analogWritePin(AnalogPin.P6, 1023)\n            } else if (mode == 1) {\n                pins.analogWritePin(AnalogPin.P5, 0)\n            }\n            SELECTED_COLOR = 0\n            // 3\n            // 3\n            basic.showString(\"1\")\n        } else if (code == 24) {\n            if (mode == 0) {\n                pins.analogWritePin(AnalogPin.P7, 1023)\n                pins.analogWritePin(AnalogPin.P5, 1023)\n                pins.analogWritePin(AnalogPin.P6, 0)\n            } else if (mode == 1) {\n                pins.analogWritePin(AnalogPin.P6, 0)\n            }\n            SELECTED_COLOR = 1\n            // 3\n            basic.showString(\"2\")\n        } else if (code == 122) {\n            if (mode == 0) {\n                pins.analogWritePin(AnalogPin.P6, 1023)\n                pins.analogWritePin(AnalogPin.P5, 1023)\n                pins.analogWritePin(AnalogPin.P7, 0)\n            } else if (mode == 1) {\n                pins.analogWritePin(AnalogPin.P7, 0)\n            }\n            SELECTED_COLOR = 2\n            // 3\n            basic.showString(\"3\")\n        } else if (code == 16) {\n            discoMode = true\n            let start = input.runningTime()\n\n            while (input.runningTime() - start < 10000) {\n                randomColorStep()\n                basic.pause(200) // fast transition (change this to 100 for faster disco!)\n            }\n\n            // After 10 seconds, turn off LED\n            pins.analogWritePin(AnalogPin.P5, 1023)\n            pins.analogWritePin(AnalogPin.P6, 1023)\n            pins.analogWritePin(AnalogPin.P7, 1023)\n            discoMode = false\n        } else if (code == 56) {\n        \t\n        } else if (code == 90) {\n        \t\n        } else if (code == 66) {\n        \t\n        } else if (code == 74) {\n        \t\n        } else if (code == 82) {\n            control.reset()\n        } else {\n        \t\n        }\n    }\n})\n","README.md":"","pxt.json":"{\n    \"name\": \"IR_RGB\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"IR Remote Receiver and Controller Kit\": \"github:BrickHackers/pxt-IR-VS1838#v2.0.0\",\n        \"IR\": \"github:ElecFreaks/pxt-ir#b98ceffc22eb54086733bff8430d5e0ab42728fb\",\n        \"power\": \"github:microbit-foundation/pxt-microbit-v2-power#v0.2.7\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":1744968590374,"editorVersion":"7.0.61","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"@PdWPo%4zr[J8*`exXw!\">i</variable><variable id=\"Vui/TMT3zVmU3uj)X;1g\">r</variable><variable id=\"}$_#_]wlzAV.n4g3?l,0\">currentColor</variable><variable id=\"@qg=;/nC}gXQlGTw!TKC\">